<?xml version="1.0" encoding="UTF-8"?>
<endpoints
    xmlns="http://java.sun.com/xml/ns/jax-ws/ri/runtime"
    version="2.0">
  
   <endpoint 
    name="HECService"                    
    implementation="eu.heliovo.hec.service.HEC" 
    wsdl="WEB-INF/wsdl/HECService.wsdl"
    service="{http://service.hec/}HECService"
    port="{http://service.hec/}HECPort"
    url-pattern="/HECService"/>
    
     <endpoint 
    name="HECProvider"                    
    implementation="eu.heliovo.hec.service.HECSoapDispatcher" 
    wsdl="WEB-INF/wsdl/HECProvider.wsdl"
    service="{http://service.hec/}HECProviderService"
    port="{http://service.hec/}HECProviderPort"
    url-pattern="/HECProvider"/>

</endpoints>

